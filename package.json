{
  "name": "selenium-with-node",
  "version": "1.0.0",
  "description": "selenium-with-node",
  "main": "",
  "scripts": {
    "test:e2e": "node src/test/*",
    "start:hub": "java -jar selenium-webdriver/selenium-server-standalone-3.8.1.jar -role hub -hubConfig hub.conf.json",
    "start:node": "java -jar selenium-webdriver/selenium-server-standalone-3.8.1.jar -role node -nodeConfig node.conf.json",
    "test:local": "karma start karma.conf.js",
    "test:multi": "karma start karma.multi.conf.js"
  },
  "author": "namgyu",
  "license": "MIT",
  "dependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.1.2",
    "selenium-webdriver": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browserify": "^15.2.0",
    "chromedriver": "^2.34.1",
    "karma": "^2.0.0",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-browserify": "^5.1.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "watchify": "^3.9.0"
  }
}

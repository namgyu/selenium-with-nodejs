{
  "name": "selenium-with-nodejs",
  "version": "1.0.0",
  "description": "selenium-with-nodejs",
  "main": "",
  "scripts": {
    "start:hub": "java -jar webdriver/selenium-server-standalone-3.8.1.jar -role hub -hubConfig config/webdriver/hub.conf.json",
    "start:node": "java -jar webdriver/selenium-server-standalone-3.8.1.jar -role node -nodeConfig config/webdriver/node.conf.json",
    "test:e2e": "node test/e2e",
    "test:unit": "karma start config/karma/karma.conf.js",
    "test:unit-multi": "karma start config/karma/karma.multi.conf.js"
  },
  "author": "namgyu",
  "license": "MIT",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "env"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "chai": "^3.5.0",
    "karma-webdriver-launcher": "^1.0.5",
    "mocha": "^3.1.2",
    "selenium-webdriver": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-espower": "^2.4.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "browserify": "^15.2.0",
    "chromedriver": "^2.34.1",
    "karma": "^2.0.0",
    "karma-browserify": "^5.1.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "power-assert": "^1.4.4",
    "watchify": "^3.9.0"
  }
}
